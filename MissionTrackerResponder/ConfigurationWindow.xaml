<UserControl x:Class="EddiMissionTrackerResponder.ConfigurationWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EddiMissionTrackerResponder"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="800">
    <StackPanel Background="#FFE5E5E5">
        <Button Content="Load Journal" Click="testButtonClick"/>
        <Button Content="Show Mission Tracker"  Click="clearStore"/>
    </StackPanel>
</UserControl>
